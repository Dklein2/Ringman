instance_create(0,0,updateCheckerObj);
}
else if (deviceManagement == 1)
{
    instance_create(0,0,touchPromptObj);
    if(adsonoff==1)
    {
        ads_enable((room_width/2)-200,(room_height - 100),0);
        analytics_event("Free User Started App");
    }
    else
    {
        analytics_event("Paid User Started App");
    }
}
else if (deviceManagement == 2)
{
    if(win8_device_touchscreen_available())
    {
    instance_create(0,0,touchPromptObj);
    }
    else
    {
    instance_create(0,0,enterPromptObj);
    }
